<h1>Members</h1>
<td><%= link_to 'New Member', new_member_path %>
<br>
</br>
<table>
  <thead>
  <tr>
  </tr>
  </thead>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
  <% @members.each do |member| %>
    <tr>

      <td><%= member.name%></td>
      <td><%= member.email%></td>
      <td><%= link_to 'Show', member %></td>
      <td><%= link_to 'Edit', edit_member_path(member) %></td>
      <td><%= link_to 'Delete', member, method: :delete, data: {confirm: 'Deseja excluir?' } %></td>
      <td></td>
    </tr>
  <% end %>
</table>
<br>